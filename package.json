{
  "name": "nuxt-ts-storybook",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint:js": "eslint --ext \".js,.ts,.vue\" --ignore-path .gitignore .",
    "lint": "yarn lint:js",
    "lintfix": "yarn lint:js -- --fix",
    "test": "jest --runInBand --no-cache",
    "test:generate-output": "jest --runInBand --no-cache --json --outputFile=.jest-test-results.json",
    "storybook": "yarn test:generate-output && npx nuxt storybook",
    "reset": "rm -rf node_modules yarn.lock && yarn"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.13.6",
    "@nuxtjs/i18n": "^7.2.0",
    "@nuxtjs/storybook": "^4.3.1",
    "@storybook/addon-a11y": "^6.5.5",
    "@storybook/addon-jest": "^6.5.5",
    "@vue/vue2-jest": "^28.0.0",
    "core-js": "^3.19.3",
    "jest-environment-jsdom": "^28.1.0",
    "jest-transform-stub": "^2.0.0",
    "nuxt": "^2.15.8",
    "vue": "^2.6.14",
    "vue-server-renderer": "^2.6.14",
    "vue-template-compiler": "^2.6.14",
    "vuetify": "^2.6.1",
    "webpack": "^5.72.1"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.16.3",
    "@nuxt/types": "^2.15.8",
    "@nuxt/typescript-build": "^2.1.0",
    "@nuxtjs/eslint-config-typescript": "^8.0.0",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/vuetify": "^1.12.3",
    "@storybook/addon-interactions": "^6.5.5",
    "@storybook/addon-viewport": "^6.5.5",
    "@storybook/jest": "^0.0.10",
    "@storybook/testing-library": "^0.0.11",
    "@types/jest": "^27.4.0",
    "@vue/test-utils": "^1.3.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^28.1.0",
    "eslint": "^8.4.1",
    "eslint-plugin-nuxt": "^3.1.0",
    "eslint-plugin-vue": "^8.2.0",
    "jest": "^28.1.0",
    "postcss": "^8.4.14",
    "postcss-import": "13.0.0",
    "postcss-loader": "4.1.0",
    "postcss-url": "10.1.1",
    "sass": "^1.52.1",
    "sass-loader": "10.1.1",
    "ts-jest": "^28.0.3",
    "ts-node": "^10.8.0"
  }
}
